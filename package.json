{
  "name": "@clawdbot/dingtalk",
  "version": "1.0.0",
  "type": "module",
  "description": "DingTalk Stream mode channel plugin for Clawdbot",
  "license": "MIT",
  "files": [
    "index.ts",
    "src",
    "clawdbot.plugin.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wendell1224/channel_dingtalk_for_clawdbot.git"
  },
  "keywords": [
    "clawdbot",
    "dingtalk",
    "钉钉",
    "chatbot",
    "ai",
    "claude"
  ],
  "clawdbot": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "dingtalk",
      "label": "DingTalk",
      "selectionLabel": "DingTalk (钉钉)",
      "docsPath": "/channels/dingtalk",
      "docsLabel": "dingtalk",
      "blurb": "DingTalk Stream mode - 无需公网IP",
      "order": 80
    },
    "install": {
      "npmSpec": "@clawdbot/dingtalk",
      "localPath": ".",
      "defaultChoice": "npm"
    }
  },
  "dependencies": {
    "@alicloud/dingtalk": "^2.0.0",
    "@alicloud/openapi-client": "^0.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "clawdbot": ">=2026.1.24",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "clawdbot": ">=2026.1.24"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  }
}
